# Test Environment Configuration

# Server Settings
PORT=9000
NODE_ENV=test

# Log Settings (set to error to reduce test output noise)
LOG_LEVEL=error

# Backend Service Mock Endpoints for API Tests
ENDPOINTS=http://localhost:9081,http://localhost:9082,http://localhost:9083

# Request Settings
TIMEOUT_MS=3000
MAX_RETRIES=3

# Health Check Settings
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL_MS=1000
FAIL_THRESHOLD=3
RECOVERY_TIME_MS=3000

# Performance Routing Settings
PERFORMANCE_BASED_ROUTING=false
