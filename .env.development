# Application Settings
NODE_ENV=development
PORT=9000

# Logging
LOG_LEVEL=debug
LOG_FILE_PATH=logs/

# Backend Services
# Multiple endpoints separated by commas
ENDPOINTS=http://localhost:8080,http://localhost:8081,http://localhost:8082

# Request Settings
TIMEOUT_MS=30000
MAX_RETRIES=3

# Health Check Settings
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL_MS=30000
FAIL_THRESHOLD=3
RECOVERY_TIME_MS=30000

# Performance Routing Settings
PERFORMANCE_BASED_ROUTING=true
